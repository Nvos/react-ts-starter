{
  "name": "boilerplate",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "bundle:stats": "react-app-rewired build --analyze",
    "test": "react-app-rewired test",
    "storybook": "start-storybook -p 9010  -s public",
    "build-storybook": "build-storybook -s public",
    "lint": "eslint src/** --ext .ts,.tsx",
    "lint:fix": "eslint src/** --ext .ts,.tsx --fix",
    "i18:add-locale": "lingui add-locale",
    "i18:extract": "lingui extract",
    "i18:compile": "lingui compile",
    "docz:dev": "docz dev",
    "docz:build": "docz build"
  },
  "dependencies": {
    "@lingui/react": "^2.7.4",
    "@loadable/component": "^5.7.0",
    "@storybook/react": "^4.1.16",
    "connected-react-router": "^6.3.2",
    "history": "^4.9.0",
    "immer": "^2.1.4",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-hot-loader": "^4.8.4",
    "react-redux": "^7.0.2",
    "react-router-dom": "^5.0.0",
    "react-scripts": "^2.1.8",
    "redux": "^4.0.1",
    "redux-observable": "^1.1.0",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0",
    "rxjs": "^6.4.0",
    "styled-components": "^4.2.0",
    "styled-system": "^4.1.0",
    "typesafe-actions": "^4.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.0",
    "@commitlint/cli": "^7.5.2",
    "@commitlint/config-conventional": "^7.5.0",
    "@hot-loader/react-dom": "^16.8.6",
    "@lingui/cli": "^2.7.4",
    "@lingui/loader": "^2.7.4",
    "@lingui/macro": "^2.7.4",
    "@storybook/addon-actions": "^5.0.8",
    "@storybook/addon-info": "^5.0.8",
    "@storybook/addon-links": "^5.0.8",
    "@storybook/addons": "^5.0.8",
    "@types/history": "^4.7.2",
    "@types/jest": "24.0.11",
    "@types/lingui__core": "^2.7.0",
    "@types/lingui__macro": "^2.7.3",
    "@types/lingui__react": "^2.5.2",
    "@types/loadable__component": "^5.6.0",
    "@types/node": "^11.11.6",
    "@types/react": "^16.8.13",
    "@types/react-dom": "^16.8.4",
    "@types/react-redux": "^7.0.6",
    "@types/react-router-dom": "^4.3.2",
    "@types/storybook__addon-actions": "^3.4.2",
    "@types/storybook__addon-links": "^3.3.4",
    "@types/storybook__react": "^4.0.1",
    "@types/styled-components": "^4.1.14",
    "@types/styled-system": "^4.0.0",
    "@typescript-eslint/eslint-plugin": "^1.6.0",
    "@typescript-eslint/parser": "^1.6.0",
    "babel-loader": "^8.0.5",
    "customize-cra": "^0.2.12",
    "cz-conventional-changelog": "^2.1.0",
    "docz": "^1.0.0",
    "docz-theme-default": "^1.0.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-import": "^2.17.1",
    "eslint-plugin-jest": "^22.4.1",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-react-hooks": "^1.6.0",
    "husky": "^1.3.1",
    "jest-image-snapshot": "^2.8.1",
    "jest-styled-components": "^6.3.1",
    "lint-staged": "^8.1.5",
    "prettier": "^1.17.0",
    "react-app-rewired": "^2.1.1",
    "react-docgen-typescript-loader": "^3.1.0",
    "react-docgen-typescript-webpack-plugin": "^1.1.0",
    "react-testing-library": "^6.1.2",
    "typescript": "3.3.4000",
    "webpack-bundle-analyzer": "^3.1.0"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "prettier --write",
      "npm run lint:fix --fix",
      "jest --findRelatedTests",
      "git add"
    ]
  }
}
