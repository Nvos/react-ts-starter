{
  "name": "boilerplate",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "bundle:stats": "react-app-rewired build --analyze",
    "test": "react-app-rewired test",
    "storybook": "start-storybook -p 9010  -s public",
    "build-storybook": "build-storybook -s public",
    "lint": "eslint src/** --ext .ts,.tsx",
    "lint:fix": "eslint src/** --ext .ts,.tsx --fix",
    "i18:add-locale": "lingui add-locale",
    "i18:extract": "lingui extract",
    "i18:compile": "lingui compile",
    "format:fix": "prettier --write"
  },
  "dependencies": {
    "@lingui/react": "^2.8.3",
    "@loadable/component": "^5.10.1",
    "@storybook/react": "^5.1.9",
    "history": "^4.9.0",
    "immer": "^3.1.3",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-hot-loader": "^4.11.1",
    "react-redux": "^7.1.0",
    "react-router-dom": "^5.0.1",
    "react-scripts": "^3.0.1",
    "redux": "^4.0.1",
    "redux-observable": "^1.1.0",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0",
    "rxjs": "^6.5.2",
    "styled-components": "^4.3.2",
    "styled-system": "^5.0.12",
    "typesafe-actions": "^5.0.0-2"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@commitlint/cli": "^8.0.0",
    "@commitlint/config-conventional": "^8.0.0",
    "@hot-loader/react-dom": "^16.8.6",
    "@lingui/cli": "^2.8.3",
    "@lingui/loader": "^2.8.3",
    "@lingui/macro": "^2.8.3",
    "@storybook/addon-actions": "^5.1.9",
    "@storybook/addon-info": "^5.1.9",
    "@storybook/addon-links": "^5.1.9",
    "@storybook/addon-options": "^5.1.9",
    "@storybook/addons": "^5.1.9",
    "@testing-library/react": "^8.0.4",
    "@types/history": "^4.7.2",
    "@types/jest": "^24.0.15",
    "@types/lingui__core": "^2.7.0",
    "@types/lingui__macro": "^2.7.3",
    "@types/lingui__react": "^2.8.0",
    "@types/loadable__component": "^5.9.0",
    "@types/node": "^12.0.10",
    "@types/react": "^16.8.22",
    "@types/react-dom": "^16.8.4",
    "@types/react-redux": "^7.1.1",
    "@types/react-router-dom": "^4.3.4",
    "@types/storybook__addon-actions": "^3.4.3",
    "@types/storybook__addon-links": "^3.3.5",
    "@types/storybook__addon-options": "^4.0.2",
    "@types/storybook__react": "^4.0.2",
    "@types/styled-components": "^4.1.16",
    "@types/styled-system": "^4.2.1",
    "@typescript-eslint/eslint-plugin": "^1.11.0",
    "@typescript-eslint/parser": "^1.11.0",
    "babel-eslint": "^10.0.2",
    "babel-loader": "^8.0.6",
    "customize-cra": "^0.2.14",
    "cz-conventional-changelog": "^2.1.0",
    "eslint": "^6.0.1",
    "eslint-config-alloy": "^2.0.4",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-jest": "^22.7.1",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.2",
    "eslint-plugin-react-hooks": "^1.6.1",
    "husky": "^2.7.0",
    "jest-image-snapshot": "^2.9.0",
    "jest-styled-components": "^6.3.3",
    "lint-staged": "^8.2.1",
    "prettier": "^1.18.2",
    "react-app-rewired": "^2.1.3",
    "react-docgen-typescript-loader": "^3.1.0",
    "react-docgen-typescript-webpack-plugin": "^1.1.0",
    "storybook-addon-jsx": "^7.1.2",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.0.1",
    "typescript": "^3.5.2",
    "webpack-bundle-analyzer": "^3.3.2"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "prettier --write",
      "npm run lint:fix --fix",
      "jest --findRelatedTests",
      "git add"
    ]
  }
}
